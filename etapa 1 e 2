function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(20);
  ellipse(200, 350, 30, 50);
  rect(200, 20, 55, 55) 
}
etapa 2
var x = 200
var y =350

function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(20);
  if(keyIsDown(RIGTH_ARROW)){
    x=x+1
    }
  if(keyIsDown(LEFT_ARROW)){
    x=x-10
  }
  
  ellipse(x, y, 50, 50)
}
  
