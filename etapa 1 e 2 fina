etapa 2
var x = 200
var y = 350

function setup () {
  createCanvas (400, 400);
}

função draw () {
  fundo (20);
  if (keyIsDown (RIGTH_ARROW)) {
    x = x + 1
    }
  if (keyIsDown (LEFT_ARROW)) {
    x = x-10
  }
  
  elipse (x, y, 50, 50)
}
